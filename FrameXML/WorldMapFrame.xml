					<KeyValue key="yspacing" value="13" type="number"/>
				</KeyValues>
				<Scripts>
					<OnLoad inherit="prepend">
						self.Tooltip.shoppingTooltips = { WorldMapCompareTooltip1, WorldMapCompareTooltip2 };
					</OnLoad>
				</Scripts>
			</Frame>
			<Frame parentKey="BackdropFrame" useParentLevel="true">
				<Anchors>
					<Anchor point="TOPLEFT"/>
				</Anchors>
			</Frame>
		</Frames>
		<Scripts>
			<OnLoad>
				local backdrop = self:GetBackdrop();
				self:SetBackdrop(nil);
				self.BackdropFrame:SetBackdrop(backdrop);
				self.BackdropFrame:SetBackdropBorderColor(TOOLTIP_DEFAULT_COLOR.r, TOOLTIP_DEFAULT_COLOR.g, TOOLTIP_DEFAULT_COLOR.b);
				self.BackdropFrame:SetBackdropColor(TOOLTIP_DEFAULT_BACKGROUND_COLOR.r, TOOLTIP_DEFAULT_BACKGROUND_COLOR.g, TOOLTIP_DEFAULT_BACKGROUND_COLOR.b);
				self.shoppingTooltips = { WorldMapCompareTooltip1, WorldMapCompareTooltip2 };
			</OnLoad>
			<OnUpdate>
				self.updateTooltip = (self.updateTooltip or TOOLTIP_UPDATE_TIME) - elapsed;
				if ( self.updateTooltip > 0 ) then
					return;
				end
				self.updateTooltip = TOOLTIP_UPDATE_TIME;

				local owner = self:GetOwner();
				if ( owner and owner.UpdateTooltip ) then
					owner:UpdateTooltip();
				end
			</OnUpdate>
			<OnHide inherit="prepend">
				WorldMapTooltip.ItemTooltip:Hide();
			</OnHide>
			<OnSizeChanged inherit="prepend">
				WorldMapTooltip_OnSizeChanged(self);
			</OnSizeChanged>
		</Scripts>
	</GameTooltip>
