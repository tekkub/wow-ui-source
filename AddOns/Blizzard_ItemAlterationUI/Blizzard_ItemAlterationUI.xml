						<Anchor point="CENTER"/>
					</Anchors>
					<TexCoords left="0.52343750" right="0.97656250" top="0.38476563" bottom="0.49609375"/>
				</Texture>
				<Texture name="$parentTransition" file="Interface\Transmogrify\Textures.png" parentKey="transition">
					<Size x="42" y="41"/>
					<Anchors>
						<Anchor point="CENTER"/>
					</Anchors>
					<TexCoords left="0.52343750" right="0.97656250" top="0.25781250" bottom="0.36914063"/>
				</Texture>
				<Texture name="$parentOuterGlowOver" parentKey="outerGlowOver" file="Interface\SpellActivationOverlay\IconAlert" alpha="0">
					<Size x="58" y="57"/>
					<Anchors>
						<Anchor point="CENTER"/>
					</Anchors>
					<TexCoords left="0.00781250" right="0.50781250" top="0.53515625" bottom="0.78515625"/>
				</Texture>
			</Layer>
		</Layers>
		<Animations>
			<AnimationGroup parentKey="anim">
				<Alpha target="$parentOuterGlowOver" duration="0.15" change="1" order="1"/>
				<Alpha target="$parentOuterGlowOver" duration="0.15" change="-1" order="2"/>
				<Scale target="$parentTransition" duration="0.2" scaleX="1.381" scaleY="1.381" order="2"/>
				<Alpha target="$parentGlow" change="-1.0" duration="0.2" order="2"/>
				<Scripts>
					<OnFinished>
						local frame = self:GetParent();
						local button = frame:GetParent();
						frame:Hide();
						button.animationFrame = nil;
						TransmogrifyFrame_UpdateSlotButton(button);
					</OnFinished>
				</Scripts>
			</AnimationGroup>
		</Animations>
	</Frame>

